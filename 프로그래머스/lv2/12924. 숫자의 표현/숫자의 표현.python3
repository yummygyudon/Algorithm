def solution(n):
    # start = 1
    answer = 1 # 자기 자신
    for i in range(1, n//2+1) :
        value = 0
        k = i
        while value < n :
            value += k
            if value == n :
                answer+=1
                break
            k += 1
     
    # while True :
    #     if 
    
    return answer